# NOVIS SDK Environment Variables
# Copy this file to .env and fill in your values

# Your wallet private key (required)
# WARNING: Never commit this file with real keys!
PRIVATE_KEY=0x_your_private_key_here

# Network settings (optional - defaults shown)
BASE_RPC_URL=https://mainnet.base.org

# Relayer (optional - defaults shown)
RELAYER_URL=https://novis-relayer-production.up.railway.app

# Pimlico API key (only needed for Smart Accounts)
PIMLICO_API_KEY=your_pimlico_api_key_here
